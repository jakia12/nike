  function onMouseFocus(e) {
    window.requestAnimationFrame(function addMouseFocus() {
      var body = document.querySelector('body');
      body.classList.add('mouse-focus');
      body.classList.remove('keyboard-focus');
    });
  }
  function onKeyboardFocus(e) {
    window.requestAnimationFrame(function addKeyboardFocus() {
      var body = document.querySelector('body');
      body.classList.add('keyboard-focus');
      body.classList.remove('mouse-focus');
    });
  }
  window.addEventListener('mousedown', onMouseFocus);
  window.addEventListener('keydown', onKeyboardFocus);
  </script><script src="https://www.nike.com/assets/experience/shop-components/public/nike-shop-client.js"></script><link href="https://static.nike.com" rel="preconnect"/><link href="https://content.services.nike.com" rel="preconnect"/><link crossorigin="true" href="https://insights-collector.newrelic.com" rel="preconnect"/><link href="https://abt.nike.com" rel="dns-prefetch"/><link href="https://unite.nike.com" rel="dns-prefetch"/><link href="https://api.nike.com" rel="dns-prefetch"/><link href="https://s3.nikecdn.com" rel="dns-prefetch"/>
        
  
