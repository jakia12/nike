          function initDotcomNav(dotcomNav) {
            dotcomNav.init({
              env: 'prod',
              clientVersion: '4.64.3',
              clientBaseUrl: 'https://www.nike.com/assets/dotcom/nav/4.64.3',
              metadata: metadata,
              messages: messages,
              initialState: initialState,
              country: 'US',
              language: 'en',
              currency: 'USD',
              locale: {"urlParam":"en","language":"en","intl":"en-US","langRegion":"en-US","default":true,"cloudUrlFragment":""},
